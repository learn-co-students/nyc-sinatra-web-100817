<form action="/landmarks/<%= @landmark.id %>" method="post">
  <input id="hidden" type="hidden" name="_method" value="patch">
  <input type="text" id="name" name="landmark[name]" value=<%= @landmark.name %>>
  <input type="number" id="year_completed" name="landmark[year_completed]" value=<%=@landmark.year_completed %>>
  <input type="submit" value="Edit Landmark">
</form>